{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "jsdelivr",
      "library": "bootstrap@4.3.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.min.js.map",
        "dist/css/bootstrap.min.css.map"
      ]
    },
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.min.map"
      ]
    },
    {
      "library": "typeahead.js@0.11.1",
      "destination": "wwwroot/lib/typeahead.js/",
      "files": [
        "typeahead.bundle.min.js"
      ]
    },
    {
      "library": "mark.js@8.11.1",
      "destination": "wwwroot/lib/mark.js/",
      "files": [
        "jquery.mark.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "brauhaus@1.1.1",
      "destination": "wwwroot/lib/brauhaus/",
      "files": [
        "lib/brauhaus.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "typeahead.js-bootstrap4-css@1.0.0",
      "destination": "wwwroot/lib/typeahead.js-bootstrap4-css/",
      "files": [
        "typeaheadjs.css"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "sortablejs@1.8.2",
      "destination": "wwwroot/lib/sortable/",
      "files": [
        "Sortable.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "clipboard@2.0.4",
      "destination": "wwwroot/lib/clipboard/",
      "files": [
        "dist/clipboard.min.js"
      ]
    }
  ]
}